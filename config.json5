{
  // Minimal OpenClaw Gateway for Zirodelta
  // Just enable the chat completions proxy endpoint
  
  "commands": {
    "native": false,
    "nativeSkills": false
  },
  
  "gateway": {
    "port": 8080,
    "bind": "lan",
    "auth": {
      "mode": "token"
    },
    "http": {
      "endpoints": {
        "chatCompletions": {
          "enabled": true
        }
      }
    }
  },
  
  "logging": {
    "level": "info",
    "consoleStyle": "json"
  }
}
